! Font settings and rendering.
! Render setting for cairo -> pango -> gtk
Xft.dpi:        96
Xft.antialias:  true
Xft.hinting:    true
Xft.rgba:       rgb
Xft.hintstyle:  hintslight

! Cursor theme
!Xcursor.theme: Oxygen Neon
!Xcursor.size: 0

! rxvt-unicode settings
URxvt.inputMethod:fcitx
URxvt.depth:32
!URxvt.externalBorder: 0
!URxvt.internalBorder: 0
!URxvt.borderLess: false
!URxvt.borderColor: #DDDDDD
URxvt.transparent: false
URxvt.urlLauncher:google-chrome
URxvt.scrollBar:False
URxvt.mouseWheelScrollPage:True
URxvt.scrollTtyOutput:False
URxvt.scrollWithBuffer:True
URxvt.scrollTtyKeypress:True
URxvt.cursorBlink:True
URxvt.cursorColor: #666666
URxvt.cursorColor2: #FFFFFF
URxvt.saveLines:1024
URxvt.letterSpace:-1
URxvt.font:xft:Consolas:size=12:style=Regular:antialias=true,xft:SimSun:size=11:style=Regular:antialias=true
URxvt.boldFont:xft:Consolas:size=12:style=Bold:antialias=true,xft:SimSun:size=11:style=Bold:antialias=true


! to match gnome-terminal "Tango" scheme
! foreground/background
URxvt*background: #000000
URxvt*foreground: #FFFFFF
! black
URxvt.color0  : #000000
URxvt.color8  : #555753
! red
URxvt.color1  : #CC0000
URxvt.color9  : #EF2929
! green
URxvt.color2  : #4E9A06
URxvt.color10 : #8AE234
! yellow
URxvt.color3  : #C4A000
URxvt.color11 : #FCE94F
! blue
URxvt.color4  : #3465A4
URxvt.color12 : #729FCF
! magenta
URxvt.color5  : #75507B
URxvt.color13 : #AD7FA8
! cyan
URxvt.color6  : #06989A
URxvt.color14 : #34E2E2
! white
URxvt.color7  : #D3D7CF
URxvt.color15 : #EEEEEC
